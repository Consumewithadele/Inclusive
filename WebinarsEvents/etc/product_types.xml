<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Catalog:etc/product_types.xsd">
    <type name="webinar" label="Webinar Product" modelInstance="Inclusive\WebinarsEvents\Model\Product\Type\EventOrWebinar" indexPriority="20" sortOrder="40">
        <priceModel instance="Inclusive\WebinarsEvents\Model\Product\Type\EventOrWebinar\Price" />
        <customAttributes>
            <attribute name="is_real_product" value="false"/>
            <attribute name="refundable" value="false"/>
        </customAttributes>
    </type>
    <type name="event" label="Event Product" modelInstance="Inclusive\WebinarsEvents\Model\Product\Type\EventOrWebinar" indexPriority="20" sortOrder="40">
        <priceModel instance="Inclusive\WebinarsEvents\Model\Product\Type\EventOrWebinar\Price" />
        <customAttributes>
            <attribute name="is_real_product" value="false"/>
            <attribute name="refundable" value="false"/>
        </customAttributes>
    </type>
    <composableTypes>
        <type name="webinar" />
        <type name="event" />
    </composableTypes>
</config>
